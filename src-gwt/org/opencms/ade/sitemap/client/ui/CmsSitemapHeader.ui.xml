<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui">
	<ui:with field="resources" type="org.opencms.ade.sitemap.client.ui.css.I_CmsSitemapLayoutBundle" />
	<g:HTMLPanel ui:field="m_panel" styleName="{resources.generalCss.cornerAll} {resources.sitemapCss.header}">
		<h2 ui:field="m_title"></h2>
		<p ui:field="m_description"></p>
		<div><b><span ui:field="m_siteHost"></span></b>&nbsp;<span ui:field="m_locale"></span></div>
	</g:HTMLPanel>
</ui:UiBinder> 