<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui">
	<ui:with field="galleryResources" type="org.opencms.ade.galleries.client.ui.css.I_CmsLayoutBundle" />
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:FlowPanel ui:field="m_tab" styleName="{resources.tabbedPanelCss.tabPanel}">
		<g:FlowPanel ui:field="m_options" addStyleNames="{galleryResources.galleryDialogCss.tabOptions}" />
		<g:ScrollPanel ui:field="m_list" styleName="{galleryResources.galleryDialogCss.criteriaList}"  addStyleNames="{resources.generalCss.cornerAll}">
		</g:ScrollPanel>			
	</g:FlowPanel> 
</ui:UiBinder> 