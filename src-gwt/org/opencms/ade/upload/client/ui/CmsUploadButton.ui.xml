	xmlns:cms="urn:import:org.opencms.gwt.client.ui">
	<ui:with field="style" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<ui:with field="css" type="org.opencms.ade.upload.client.ui.css.I_CmsLayoutBundle" />
	<cms:CmsFlowPanel tag="div" ui:field="m_main" styleName="{style.buttonCss.cmsState}" addStyleNames="{css.uploadCss.uploadButton}">
